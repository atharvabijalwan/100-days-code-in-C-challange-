// Q135 - Enum with explicit values starting from 10; print them.
#include <stdio.h>

enum Code { A=10, B, C, D, E };

int main_Q135(void){
    printf("%d %d %d %d %d\n", A, B, C, D, E);
    return 0;
}
int main135(){ return main_Q135(); }

