// Q136 - Enum menu: ADD, SUBTRACT, MULTIPLY; perform the chosen operation.
#include <stdio.h>

enum Menu { ADD=1, SUBTRACT, MULTIPLY };

int main_Q136(void){
    int c; long long a,b;
    scanf("%d %lld %lld",&c,&a,&b);

    switch(c){
        case ADD: printf("%lld\n", a+b); break;
        case SUBTRACT: printf("%lld\n", a-b); break;
        case MULTIPLY: printf("%lld\n", a*b); break;
        default: printf("Invalid\n");
    }
    return 0;
}
int main136(){ return main_Q136(); }

// --------------------------------------------------------------

// Q137 - Enum for user roles (ADMIN, USER, GUEST) and print a message.
#include <stdio.h>

enum Role { ADMIN=1, USER, GUEST };

int main_Q137(void){
    int r; scanf("%d",&r);
    switch(r){
        case ADMIN: printf("Welcome, Admin.\n"); break;
        case USER: printf("Welcome, User.\n"); break;
        case GEST: printf("Welcome, Guest.\n"); break;
        default: printf("Invalid\n");
    }
    return 0;
}
int main137(){ return main_Q137(); }

