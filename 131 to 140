// Q132 - Enum for traffic lights (RED, YELLOW, GREEN). Print Stop/Wait/Go.
#include <stdio.h>

enum Light { RED, YELLOW, GREEN };

int main_Q132(void){
    int x; scanf("%d",&x);
    switch(x){
        case RED: printf("Stop\n"); break;
        case YELLOW: printf("Wait\n"); break;
        case GREEN: printf("Go\n"); break;
        default: printf("Invalid\n");
    }
    return 0;
}
int main132(){ return main_Q132(); }

// --------------------------------------------------------------

// Q133 - Enum for months (JAN–DEC). Print how many days each month has.
#include <stdio.h>

enum Month { JAN=1, FEB, MAR, APR, MAY, JUN, JUL, AUG, SEP, OCT, NOV, DEC };

int main_Q133(void){
    int m; scanf("%d",&m);
    int days;
    switch(m){
        case JAN: days=31; break;
        case FEB: days=28; break;
        case MAR: days=31; break;
        case APR: days=30; break;
        case MAY: days=31; break;
        case JUN: days=30; break;
        case JUL: days=31; break;
        case AUG: days=31; break;
        case SEP: days=30; break;
        case OCT: days=31; break;
        case NOV: days=30; break;
        case DEC: days=31; break;
        default: printf("Invalid\n"); return 0;
    }
    printf("%d\n", days);
    return 0;
}
int main133(){ return main_Q133(); }

