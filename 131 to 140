// Q139 - Show enum values (demonstrate they are integers).
#include <stdio.h>

enum Example { X=5, Y=10, Z=25 };

int main_Q139(void){
    printf("%d %d %d\n", X, Y, Z);
    return 0;
}
int main139(){ return main_Q139(); }

// --------------------------------------------------------------

// Q140 - Struct with enum Gender; print person’s gender.
#include <stdio.h>

enum Gender { MALE, FEMALE, OTHER };

struct Person {
    char name[100];
    int age;
    enum Gender g;
};

int main_Q140(void){
    struct Person p;
    scanf("%s %d %d", p.name, &p.age, (int*)&p.g);

    if(p.g == MALE) printf("%s is Male\n", p.name);
    else if(p.g == FEMALE) printf("%s is Female\n", p.name);
    else printf("%s is Other\n", p.name);

    return 0;
}
int main140(){ return main_Q140(); }

