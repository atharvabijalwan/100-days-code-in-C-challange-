// Q131 - Enum for days (SUNDAY to SATURDAY) and print their integer values.
#include <stdio.h>

enum Day { SUNDAY, MONDAY, TUESDAY, WEDNESDAY, THURSDAY, FRIDAY, SATURDAY };

int main_Q131(void){
    for(int d=SUNDAY; d<=SATURDAY; d++)
        printf("%d\n", d);
    return 0;
}
int main(){ return main_Q131(); }

