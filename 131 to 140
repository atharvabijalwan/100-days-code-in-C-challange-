// Q138 - Print all enum integer values using a loop.
#include <stdio.h>

enum Days2 { D_SUN, D_MON, D_TUE, D_WED, D_THU, D_FRI, D_SAT };

int main_Q138(void){
    for(int i=D_SUN; i<=D_SAT; i++)
        printf("%d\n", i);
    return 0;
}
int main138(){ return main_Q138(); }

