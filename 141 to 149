// Q142 - Store details of 5 students in array of structures and print all.
#include <stdio.h>

struct Stud {
    char name[100];
    int roll;
    float marks;
};

int main_Q142(void){
    struct Stud a[5];
    for(int i=0;i<5;i++)
        scanf("%s %d %f", a[i].name, &a[i].roll, &a[i].marks);

    for(int i=0;i<5;i++)
        printf("%s %d %.2f\n", a[i].name, a[i].roll, a[i].marks);

    return 0;
}
int main142(){ return main_Q142(); }

// --------------------------------------------------------------

// Q143 - Find and print the student with the highest marks.
#include <stdio.h>
#include <stdlib.h>

struct S143 {
    char name[100];
    int roll;
    float marks;
};

int main_Q143(void){
    int n; scanf("%d",&n);
    struct S143 *a = malloc(n * sizeof(struct S143));

    for(int i=0;i<n;i++)
        scanf("%s %d %f", a[i].name, &a[i].roll, &a[i].marks);

    int best = 0;
    for(int i=1;i<n;i++)
        if(a[i].marks > a[best].marks)
            best = i;

    printf("%s %d %.2f\n", a[best].name, a[best].roll, a[best].marks);
    free(a);
    return 0;
}
int main143(){ return main_Q143(); }

