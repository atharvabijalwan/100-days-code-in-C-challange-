// Q146 - Employee structure with nested Date structure.
#include <stdio.h>

struct Date146 { int d, m, y; };

struct Emp146 {
    char name[100];
    int id;
    struct Date146 join;
};

int main_Q146(void){
    struct Emp146 e;
    scanf("%s %d %d %d %d", e.name, &e.id, &e.join.d, &e.join.m, &e.join.y);
    printf("%s %d %02d-%02d-%04d\n", e.name, e.id, e.join.d, e.join.m, e.join.y);
    return 0;
}
int main146(){ return main_Q146(); }

// --------------------------------------------------------------

// Q147 - Store employee data in a binary file using fwrite() and read using fread().
#include <stdio.h>

struct EMP147 {
    char name[100];
    int id;
    double salary;
};

int main_Q147(void){
    int n; scanf("%d",&n);
    struct EMP147 e;

    FILE *fw = fopen("employees.bin","wb");
    if(!fw){ printf("Error\n"); return 0; }

    for(int i=0;i<n;i++){
        scanf("%s %d %lf", e.name, &e.id, &e.salary);
        fwrite(&e, sizeof(e), 1, fw);
    }
    fclose(fw);

    FILE *fr = fopen("employees.bin","rb");
    if(!fr){ printf("Error\n"); return 0; }

    while(fread(&e, sizeof(e), 1, fr) == 1)
        printf("%s %d %.2f\n", e.name, e.id, e.salary);

    fclose(fr);
    return 0;
}
int main147(){ return main_Q147(); }

// --------------------------------------------------------------
// Q148 - Take two structs as input and check if they are identical.
#include <stdio.h>
#include <string.h>

struct Data148 {
    char name[100];
    int id;
    float score;
};

int main_Q148(void){
    struct Data148 a, b;

    // Input format example:
    // name id score
    // name id score
    scanf("%s %d %f", a.name, &a.id, &a.score);
    scanf("%s %d %f", b.name, &b.id, &b.score);

    if(strcmp(a.name, b.name)==0 && a.id == b.id && a.score == b.score)
        printf("Identical\n");
    else
        printf("Not Identical\n");

    return 0;
}
int main(){ return main_Q148(); }

// --------------------------------------------------------------
// Q149 - Use malloc() to allocate structure memory dynamically and print details.

#include <stdio.h>

#include <stdlib.h>



struct Student149 {

    char name[100];

    int roll;

    float marks;

};



int main_Q149(void){

    struct Student149 *s;



    // Allocate memory dynamically

    s = (struct Student149*)malloc(sizeof(struct Student149));

    if(s == NULL){

        printf("Memory Allocation Failed\n");

        return 0;

    }



    // Input format: name roll marks

    scanf("%s %d %f", s->name, &s->roll, &s->marks);



    // Print details

    printf("%s %d %.2f\n", s->name, s->roll, s->marks);



    free(s); // release memory

    return 0;

}

int main(){ return main_Q149(); }



