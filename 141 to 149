// Q144 - Function that accepts a structure and prints its members.
#include <stdio.h>

struct P144 {
    char name[100];
    int id;
    float score;
};

void printP144(struct P144 x){
    printf("%s %d %.2f\n", x.name, x.id, x.score);
}

int main_Q144(void){
    struct P144 p;
    scanf("%s %d %f", p.name, &p.id, &p.score);
    printP144(p);
    return 0;
}
int main144(){ return main_Q144(); }

// --------------------------------------------------------------

// Q145 - Return a structure (top student) from a function.
#include <stdio.h>
#include <stdlib.h>

struct ST145 {
    char name[100];
    int roll;
    float marks;
};

struct ST145 topStudent(struct ST145 arr[], int n){
    int best = 0;
    for(int i=1;i<n;i++)
        if(arr[i].marks > arr[best].marks)
            best = i;
    return arr[best];
}

int main_Q145(void){
    int n; scanf("%d",&n);
    struct ST145 *a = malloc(n * sizeof(struct ST145));

    for(int i=0;i<n;i++)
        scanf("%s %d %f", a[i].name, &a[i].roll, &a[i].marks);

    struct ST145 t = topStudent(a, n);
    printf("%s %d %.2f\n", t.name, t.roll, t.marks);

    free(a);
    return 0;
}
int main145(){ return main_Q145(); }

