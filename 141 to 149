// Q141 - Structure Student (name, roll_no, marks). Read and print one student.
#include <stdio.h>

struct Student {
    char name[100];
    int roll_no;
    float marks;
};

int main_Q141(void){
    struct Student s;
    scanf("%s %d %f", s.name, &s.roll_no, &s.marks);
    printf("%s %d %.2f\n", s.name, s.roll_no, s.marks);
    return 0;
}
int main(){ return main_Q141(); }

